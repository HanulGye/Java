package com.hanul.io;

public class Thread_2 extends Thread {

	String str;
	public Thread_2(String s) {
		this.str = s;
	}
	
	public void run() {
		for(int i =0;i<10;i++) {
			
			System.out.println(str);
			
		}
		
	}
	
	public static void main(String[] args) {
		System.out.println("hello");
	}
	
}
